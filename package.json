{
  "name": "supa-storage",
  "version": "0.0.1",
  "description": "Supabase storage middleend",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --log-error --files ./src/server.ts",
    "build": "tsc -p tsconfig.json",
    "pkg": "pkg dist/server.js",
    "start": "NODE_ENV=production node dist/server.js",
    "migration:run": "ts-node-dev ./src/scripts/migrate-call.ts",
    "docs:export": "ts-node-dev ./src/scripts/export-docs.ts",
    "test:dummy-data": "ts-node-dev -r dotenv/config ./src/test/db/import-dummy-data.ts",
    "test": "npm run infra:restart && npm run test:dummy-data && jest --runInBand",
    "prettier:check": "prettier -c src/**",
    "eslint:check": "eslint 'src/**'",
    "infra:stop": "docker-compose --project-dir . -f src/test/db/docker-compose.yml down  --remove-orphans",
    "infra:start": "docker-compose --project-dir . -f src/test/db/docker-compose.yml up -d && sleep 5 && npm run migration:run",
    "infra:restart": "npm run infra:stop && npm run infra:start"
  },
  "author": "Supabase",
  "license": "ISC",
  "engines": {
    "node": ">= 12.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.8.0",
    "@aws-sdk/lib-storage": "^3.8.0",
    "@aws-sdk/node-http-handler": "^3.6.1",
    "@supabase/postgrest-js": "^0.28.4",
    "dotenv": "^8.2.0",
    "fastify": "^3.14.0",
    "fastify-cors": "^5.2.0",
    "fastify-multipart": "^4.0.1",
    "fastify-swagger": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "pkg": "^4.4.9",
    "postgres-migrations": "^5.1.1"
  },
  "devDependencies": {
    "@types/busboy": "^0.2.3",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.33",
    "@types/pg": "^7.14.11",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "form-data": "^4.0.0",
    "jest": "^26.6.3",
    "json-schema-to-ts": "^1.6.0",
    "pino-pretty": "^4.7.1",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  }
}
